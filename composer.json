{
    "name": "softon/sms",
    "description": "Simple SMS Package for sending short text messages from your Application. Currently supported Gateways MVaayoo, Gupshup, SmsAchariya. Log gateway can be used for testing.",
    "license": "MIT",
    "keywords": [
        "gupshup",
        "mvaayoo",
        "sms achariya",
        "sms gateway",
    ],
    "authors": [
        {
            "name": "Shiburaj",
            "email": "powerupneo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "guzzlehttp/guzzle": "5.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Softon\\Sms\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
